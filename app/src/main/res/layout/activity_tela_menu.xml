<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frmmenu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/green"
    tools:context=".TelaMenu">

    <View
        android:id="@+id/view"
        android:layout_width="364dp"
        android:layout_height="773dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="13dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="VisualLintBounds" />

    <ImageView
        android:id="@+id/imgCircle"
        android:layout_width="247dp"
        android:layout_height="259dp"
        android:layout_marginStart="73dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="672dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baseline_circle_24"
        tools:ignore="ContentDescription,HardcodedText,ImageContrastCheck,VisualLintBounds" />

    <ImageView
        android:id="@+id/imgLogoMenu"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="153dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="761dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_recycling_24"
        tools:ignore="ImageContrastCheck" />

    <ScrollView
        android:layout_width="364dp"
        android:layout_height="655dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imgOquee"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="10dp"
                android:importantForAccessibility="no"
                android:onClick="IrOquee"
                android:paddingStart="2dp"
                android:src="@drawable/o_que__"
                tools:ignore="ImageContrastCheck,RtlSymmetry,SpeakableTextPresentCheck,UsingOnClickInXml" />

            <ImageButton
                android:id="@+id/imgCuriosidades"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="10dp"
                android:onClick="IrCuriosidades"
                android:paddingTop="10dp"
                android:src="@drawable/curiosidades1"
                tools:ignore="ContentDescription,ImageContrastCheck,UsingOnClickInXml,SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/imgComoImpac"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="10dp"
                android:onClick="IrImpactos"
                android:src="@drawable/impactos"
                tools:ignore="ContentDescription,UsingOnClickInXml,SpeakableTextPresentCheck,ImageContrastCheck" />

            <ImageButton
                android:id="@+id/imgOndeImpac"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="10dp"
                android:onClick="IrLugarImpacto"
                android:src="@drawable/ondeimpactam"
                tools:ignore="ContentDescription,ImageContrastCheck,SpeakableTextPresentCheck,UsingOnClickInXml" />

            <ImageButton
                android:id="@+id/imgDesc"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="10dp"
                android:onClick="IrDescarte"
                android:src="@drawable/lugar"
                tools:ignore="ContentDescription,ImageContrastCheck,SpeakableTextPresentCheck,UsingOnClickInXml" />

            <ImageButton
                android:id="@+id/imgContato"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="10dp"
                android:importantForAccessibility="no"
                android:onClick="IrContato"
                android:src="@drawable/contato"
                tools:ignore="UsingOnClickInXml,ImageContrastCheck" />

            <ImageButton
                android:id="@+id/imgSensor"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="10dp"
                android:importantForAccessibility="no"
                android:onClick="IrPontoDesc"
                android:src="@drawable/descrapido"
                tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck,UsingOnClickInXml" />
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>